      "id": "call-openai-tasks",
      "name": "Call OpenAI (Tasks)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1300, 200],
      "notes": "HTTP Request node - expressions work correctly per item"
