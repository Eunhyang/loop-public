      "parameters": {
        "url": "https://mcp.sosilab.synology.me/api/strategy/context",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
